    {sub==='calc' && <><PromptBlock title="PROMPT 4A ‚Äî Generador de Calculadora HTML+JS (v3.0)" note="USO: Uno por cada calculadora. Repite para cada nueva." prompt={P4A}/><InfoBox color="#3b82f6"><p style={{color:'#60a5fa',fontSize:13,margin:0}}>‚è± <strong>5-8 min</strong> por calculadora ‚Üí Guarda como <code style={{color:'#60a5fa'}}>[url_amigable].html</code></p></InfoBox></>}
    {sub==='blog' && <><PromptBlock title="PROMPT 4B ‚Äî Generador de Blog HTML+SEO (v3.0)" note="USO: Uno por cada art√≠culo. Repite para cada nuevo blog." prompt={P4B}/><InfoBox color="#8b5cf6"><p style={{color:'#a78bfa',fontSize:13,margin:0}}>‚è± <strong>8-10 min</strong> por art√≠culo ‚Üí Guarda como <code style={{color:'#a78bfa'}}>blog-[url_amigable].html</code></p></InfoBox></>}
    {sub==='tool' && <><PromptBlock title="PROMPT 4C ‚Äî Generador de Herramienta/Comparador (v3.0)" note="USO: Para herramientas, comparadores, p√°ginas interactivas." prompt={P4C}/><InfoBox color="#ec4899"><p style={{color:'#f472b6',fontSize:13,margin:0}}>‚è± <strong>10-12 min</strong> por herramienta ‚Üí Guarda como <code style={{color:'#f472b6'}}>[url_amigable].html</code></p></InfoBox></>}
  </>);
}

