function Phase7() {
  const cats = [
    {t:"Funcionalidad",i:["Todas las calculadoras funcionan (prob√© con n√∫meros reales)","Resultados son matem√°ticamente precisos","Validaci√≥n de inputs funciona","Se ve bien en m√≥vil (375px)","Se ve bien en desktop (1200px+)","Header/Footer se carga en TODAS las p√°ginas","No hay 404 errors"]},
    {t:"SEO",i:["Cada p√°gina tiene <title> √∫nico con KW","Cada p√°gina tiene <meta description> √∫nico","Una sola <h1> por p√°gina","H2/H3 estructurados l√≥gicamente","URLs amigables","Schema JSON-LD en cada p√°gina","Images con alt text","Sitemap.xml creado","Internal links funcionan"]},
    {t:"Contenido",i:["Sin errores ortogr√°ficos","Copy profesional","Links internos funcionan","CTAs claros y visibles","Informaci√≥n verificable","P√°gina legal incluida"]},
    {t:"Rendimiento",i:["Sitio carga en < 3 seg","Sin errores en consola","Sin dependencias externas","CSS es minimal"]},
    {t:"Legal",i:["Pol√≠tica de privacidad presente","T√©rminos de servicio presentes","Aviso legal presente","Links a legales en footer"]},
  ];
  const [ch, setCh] = useState({});
  const total = cats.reduce((s,c)=>s+c.i.length,0);
  const done = Object.values(ch).filter(Boolean).length;
  const pct = Math.round((done/total)*100);

  return (<>
    <SectionTitle>üéØ Objetivo</SectionTitle>
    <p style={{color:'#94a3b8',fontSize:14,lineHeight:1.8}}>Revisar <strong style={{color:'#f59e0b'}}>TODO</strong> el sitio con el checklist de QA y <strong style={{color:'#22c55e'}}>publicar en Cloudflare Pages</strong>.</p>
    <SectionTitle>‚úÖ Checklist de QA ({done}/{total})</SectionTitle>
    <div style={{marginBottom:16}}>
      <div style={{height:8,background:'rgba(255,255,255,.05)',borderRadius:4}}><div style={{height:'100%',width:`${pct}%`,background:pct===100?'#22c55e':'linear-gradient(90deg,#f59e0b,#ef4444)',borderRadius:4,transition:'width .4s'}}/></div>
      <p className="mono" style={{color:pct===100?'#22c55e':'#64748b',fontSize:12,marginTop:6}}>{pct}% completado</p>
    </div>
    {cats.map((c,ci)=><div key={ci} style={{marginBottom:20}}>
      <h4 style={{color:'#cbd5e1',fontSize:14,marginBottom:6}}>{c.t}</h4>
      {c.i.map((item,ii)=>{const k=`${ci}-${ii}`;return <CheckItem key={k} label={item} checked={!!ch[k]} onChange={()=>setCh(p=>({...p,[k]:!p[k]}))}/> })}
    </div>)}
    <SectionTitle>üìã Estructura Final de Carpetas</SectionTitle>
    <pre className="code" style={{background:'rgba(0,0,0,.4)',border:'1px solid rgba(255,255,255,.05)',borderRadius:10,padding:18,color:'#94a3b8',fontSize:13,lineHeight:1.8,overflowX:'auto'}}>{`/tu-sitio-seo/
‚îú‚îÄ‚îÄ index.html
‚îú‚îÄ‚îÄ header-footer.html
‚îú‚îÄ‚îÄ calculadoras/
‚îÇ   ‚îú‚îÄ‚îÄ hypothekenrechner.html
‚îÇ   ‚îî‚îÄ‚îÄ ...
‚îú‚îÄ‚îÄ blog/
‚îÇ   ‚îú‚îÄ‚îÄ blog-calcular-cuota.html
‚îÇ   ‚îî‚îÄ‚îÄ ...
‚îú‚îÄ‚îÄ herramientas/
‚îÇ   ‚îî‚îÄ‚îÄ comparador.html
‚îú‚îÄ‚îÄ legal/
‚îÇ   ‚îú‚îÄ‚îÄ politica-privacidad.html
‚îÇ   ‚îú‚îÄ‚îÄ terminos-servicio.html
‚îÇ   ‚îî‚îÄ‚îÄ aviso-legal.html
‚îú‚îÄ‚îÄ sitemap.xml
‚îî‚îÄ‚îÄ robots.txt`}</pre>
    <SectionTitle>üöÄ Publicar en Cloudflare (5-10 min)</SectionTitle>
    <StepList steps={["Abre cloudflare.com","Pages ‚Üí Crear Proyecto","Direct Upload ‚Üí Selecciona TODOS tus archivos","Deploy","Conecta tu dominio"]} />
  </>);
}

const SCREENS = [Phase1, Phase2, Phase3, Phase4, Phase5, Phase6, Phase7];

// ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
// MAIN APP
// ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
function App() {
  const [completed, setCompleted] = useState(() => {
    try { return JSON.parse(localStorage.getItem('sop_completed') || '[]'); } catch { return []; }
  });
  const [active, setActive] = useState(null);
  const [badge, setBadge] = useState(null);

  useEffect(() => { try { localStorage.setItem('sop_completed', JSON.stringify(completed)); } catch {} }, [completed]);

  const xp = completed.reduce((s,id) => s + (PHASES.find(p=>p.id===id)?.xp||0), 0);
  const pct = Math.round((xp/TOTAL_XP)*100);
  const level = completed.length===0?"NOVATO":completed.length<=2?"APRENDIZ":completed.length<=4?"PRO":completed.length<=6?"EXPERTO":"MAESTRO SEO";
  const allDone = completed.length === PHASES.length;

  const complete = (id) => {
    if(completed.includes(id)) return;
    setCompleted(p=>[...p,id]);
    setBadge(PHASES.find(ph=>ph.id===id));
  };

  useEffect(() => { window.scrollTo({top:0,behavior:'smooth'}); }, [active]);

  // ‚îÄ‚îÄ PHASE DETAIL VIEW ‚îÄ‚îÄ
  if (active !== null) {
    const phase = PHASES.find(p=>p.id===active);
    const done = completed.includes(phase.id);
    const Screen = SCREENS[phase.id-1];
    const next = PHASES.find(p=>p.id===phase.id+1);

    return (
      <div>
        {badge && <BadgePopup badge={badge.badge} icon={badge.badgeIcon} onClose={()=>setBadge(null)}/>}
        <div className="nav-bar">
          <button className="back-btn" onClick={()=>setActive(null)}>‚Üê Volver al mapa</button>
          <div style={{display:'flex',alignItems:'center',gap:12}}>
            <span className="mono" style={{color:'#f59e0b',fontSize:12,fontWeight:700}}>{xp} XP</span>
            <div style={{width:80,height:6,background:'rgba(255,255,255,.06)',borderRadius:3}}><div style={{height:'100%',width:`${pct}%`,background:'linear-gradient(90deg,#f59e0b,#ef4444)',borderRadius:3,transition:'width .5s'}}/></div>
          </div>
        </div>
        <div style={{padding:'48px 20px 0',maxWidth:780,margin:'0 auto'}}>
          <div style={{display:'flex',alignItems:'center',gap:14,marginBottom:8}}>
            <span style={{fontSize:36}}>{phase.icon}</span>
            <div>
              <div className="mono" style={{color:'#64748b',fontSize:12,fontWeight:700,letterSpacing:2}}>FASE {phase.id} DE {PHASES.length}</div>
              <h1 style={{fontSize:'clamp(22px,4vw,32px)',fontWeight:800,color:'#f1f5f9',lineHeight:1.2,marginTop:4}}>{phase.title}</h1>
            </div>
          </div>
          <p style={{color:'#64748b',fontSize:15,marginBottom:12}}>{phase.subtitle}</p>
          <div style={{display:'flex',gap:10,flexWrap:'wrap',marginBottom:32}}>
            <span className="mono" style={{background:'rgba(255,255,255,.04)',border:'1px solid rgba(255,255,255,.08)',borderRadius:8,padding:'5px 14px',fontSize:12,color:'#64748b'}}>‚è± {phase.time}</span>
            <span className="mono" style={{background:done?'rgba(34,197,94,.1)':'rgba(245,158,11,.1)',border:`1px solid ${done?'rgba(34,197,94,.2)':'rgba(245,158,11,.2)'}`,borderRadius:8,padding:'5px 14px',fontSize:12,color:done?'#4ade80':'#f59e0b',fontWeight:700}}>{done?"‚úì COMPLETADA":`+${phase.xp} XP`}</span>
            <span className="mono" style={{background:'rgba(255,255,255,.04)',border:'1px solid rgba(255,255,255,.08)',borderRadius:8,padding:'5px 14px',fontSize:12,color:'#64748b'}}>üéØ {phase.output}</span>
          </div>
        </div>
        <div style={{maxWidth:780,margin:'0 auto',padding:'0 20px 60px',animation:'fadeIn .4s'}}>
          <Screen/>
          <div style={{marginTop:40,display:'flex',flexDirection:'column',gap:12}}>
            {!done && <button className="complete-btn" onClick={()=>complete(phase.id)} style={{background:'linear-gradient(135deg,#f59e0b,#d97706)'}}>‚úÖ COMPLETAR FASE {phase.id} ‚Äî RECLAMAR +{phase.xp} XP</button>}
            {done && next && <button className="complete-btn" onClick={()=>setActive(next.id)} style={{background:'linear-gradient(135deg,#22c55e,#16a34a)',color:'#fff'}}>‚Üí AVANZAR A FASE {next.id}: {next.title}</button>}
            {done && !next && <button className="complete-btn" onClick={()=>setActive(null)} style={{background:'linear-gradient(135deg,#22c55e,#16a34a)',color:'#fff'}}>üèÜ VER RESUMEN FINAL</button>}
          </div>
        </div>
      </div>
    );
  }

  // ‚îÄ‚îÄ MAP / HUB VIEW ‚îÄ‚îÄ
  return (
    <div>
      {badge && <BadgePopup badge={badge.badge} icon={badge.badgeIcon} onClose={()=>setBadge(null)}/>}

      {/* HERO */}
      <div style={{background:'linear-gradient(170deg,#0c1220,#0a0f1a 40%,#111827)',borderBottom:'1px solid rgba(255,255,255,.04)',padding:'56px 20px 52px',textAlign:'center',position:'relative',overflow:'hidden'}}>
        <div className="hero-grid"/>
        <div style={{position:'relative',maxWidth:700,margin:'0 auto'}}>
          <div className="mono" style={{display:'inline-block',background:'rgba(245,158,11,.1)',border:'1px solid rgba(245,158,11,.25)',borderRadius:8,padding:'6px 16px',fontSize:12,fontWeight:700,color:'#f59e0b',letterSpacing:2,marginBottom:20}}>PROFESSIONAL v2</div>
          <h1 style={{fontSize:'clamp(28px,5vw,44px)',fontWeight:800,lineHeight:1.15,marginBottom:16,background:'linear-gradient(135deg,#f1f5f9,#94a3b8)',WebkitBackgroundClip:'text',WebkitTextFillColor:'transparent'}}>Nichos SEO de Calculadoras</h1>
          <p style={{fontSize:'clamp(15px,2.5vw,18px)',color:'#64748b',marginBottom:36,lineHeight:1.6}}>De <strong style={{color:'#f59e0b'}}>IDEA</strong> a <strong style={{color:'#22c55e'}}>SITIO PUBLICADO</strong> en 2.5-3 Horas</p>

          <div style={{maxWidth:420,margin:'0 auto'}}>
            <div style={{display:'flex',justifyContent:'space-between',alignItems:'baseline',marginBottom:8}}>
              <span className="mono" style={{color:'#f59e0b',fontWeight:700,fontSize:14,animation:completed.length>0?'glow 2s infinite':'none'}}>LVL: {level}</span>
              <span className="mono" style={{color:'#64748b',fontSize:13}}>{xp}/{TOTAL_XP} XP</span>
            </div>
            <div style={{height:10,background:'rgba(255,255,255,.04)',borderRadius:5,border:'1px solid rgba(255,255,255,.06)',overflow:'hidden'}}>
              <div style={{height:'100%',width:`${pct}%`,background:pct===100?'linear-gradient(90deg,#22c55e,#16a34a)':'linear-gradient(90deg,#f59e0b,#ef4444,#ec4899)',borderRadius:5,transition:'width .8s cubic-bezier(.22,1,.36,1)',boxShadow:pct>0?'0 0 12px rgba(245,158,11,.3)':'none'}}/>
            </div>
            <div className="mono" style={{display:'flex',justifyContent:'space-between',marginTop:6,fontSize:11,color:'#475569'}}>
              <span>{completed.length}/{PHASES.length} fases</span><span>{pct}%</span>
            </div>
          </div>

          {completed.length > 0 && <div style={{marginTop:20,display:'flex',justifyContent:'center',gap:8,flexWrap:'wrap'}}>
            {completed.map(id=>{const p=PHASES.find(ph=>ph.id===id);return <div key={id} className="mono" style={{background:'rgba(245,158,11,.1)',border:'1px solid rgba(245,158,11,.2)',borderRadius:8,padding:'4px 12px',fontSize:11,color:'#f59e0b',fontWeight:700}}>{p.badgeIcon} {p.badge}</div>})}
          </div>}
        </div>
      </div>

      {/* PHASE LIST ‚Äî only show completed + next unlocked */}
      <div style={{maxWidth:680,margin:'0 auto',padding:'40px 20px 80px'}}>
        {PHASES.map((phase,idx) => {
          const done = completed.includes(phase.id);
          const unlocked = phase.id===1 || completed.includes(phase.id-1);
          const isNext = !done && unlocked;
          if (!done && !isNext) return null;

          return (
            <div key={phase.id} style={{position:'relative',paddingLeft:52,marginBottom:16}}>
              {idx < PHASES.length-1 && done && <div style={{position:'absolute',left:21,top:48,bottom:-16,width:2,background:'rgba(34,197,94,.25)'}}/>}
              <div style={{position:'absolute',left:8,top:16,width:28,height:28,borderRadius:'50%',background:done?'#22c55e':'#f59e0b',display:'flex',alignItems:'center',justifyContent:'center',fontSize:13,color:done?'#fff':'#000',fontWeight:800,fontFamily:"'Space Mono',monospace",animation:isNext?'pulse 2s infinite':'none'}}>{done?"‚úì":phase.id}</div>
              <div className="phase-card" onClick={()=>setActive(phase.id)} style={{background:isNext?'linear-gradient(135deg,rgba(245,158,11,.08),rgba(245,158,11,.02))':'rgba(34,197,94,.03)',border:isNext?'1px solid rgba(245,158,11,.25)':'1px solid rgba(34,197,94,.12)'}}>
                <div style={{display:'flex',alignItems:'center',justifyContent:'space-between',flexWrap:'wrap',gap:10}}>
                  <div style={{display:'flex',alignItems:'center',gap:12}}>
                    <span style={{fontSize:24}}>{phase.icon}</span>
                    <div>
                      <div style={{fontSize:16,fontWeight:700,color:done?'#4ade80':'#e2e8f0'}}>Fase {phase.id}: {phase.title}</div>
                      <div style={{fontSize:12,color:'#64748b',marginTop:2}}>{phase.subtitle}</div>
                    </div>
                  </div>
                  <div style={{display:'flex',alignItems:'center',gap:8}}>
                    <span className="mono" style={{background:'rgba(255,255,255,.04)',border:'1px solid rgba(255,255,255,.08)',borderRadius:6,padding:'4px 12px',fontSize:11,color:'#64748b'}}>‚è± {phase.time}</span>
                    <span className="mono" style={{background:done?'rgba(34,197,94,.1)':'rgba(245,158,11,.1)',border:`1px solid ${done?'rgba(34,197,94,.2)':'rgba(245,158,11,.2)'}`,borderRadius:6,padding:'4px 12px',fontSize:11,color:done?'#4ade80':'#f59e0b',fontWeight:700}}>{done?"‚úì DONE":`+${phase.xp} XP`}</span>
                    <span style={{color:'#475569',fontSize:16,marginLeft:4}}>‚Üí</span>
                  </div>
                </div>
              </div>
            </div>
          );
        })}

        {/* Final summary */}
        {allDone && <div style={{marginTop:48,padding:'44px 32px',background:'linear-gradient(145deg,rgba(34,197,94,.06),rgba(245,158,11,.04))',border:'2px solid rgba(34,197,94,.2)',borderRadius:24,textAlign:'center',animation:'fadeIn .6s'}}>
          <div style={{fontSize:60,marginBottom:16}}>üèÜ</div>
          <h2 className="mono" style={{fontSize:28,fontWeight:800,marginBottom:8,background:'linear-gradient(135deg,#22c55e,#f59e0b)',WebkitBackgroundClip:'text',WebkitTextFillColor:'transparent'}}>¬°MISI√ìN COMPLETA!</h2>
          <p style={{color:'#94a3b8',fontSize:15,marginBottom:32}}>Has completado las 7 fases. Tu sitio SEO de calculadoras est√° listo.</p>
          <div style={{display:'grid',gridTemplateColumns:'repeat(auto-fit,minmax(140px,1fr))',gap:12,marginBottom:32}}>
            {["Investigaci√≥n","100+ Keywords","N URLs creadas","SEO perfecto","Legal completo","Publicado"].map((l,i)=>
              <div key={i} style={{background:'rgba(34,197,94,.06)',border:'1px solid rgba(34,197,94,.12)',borderRadius:10,padding:12,fontSize:12,color:'#4ade80',fontWeight:600}}>‚úÖ {l}</div>
            )}
          </div>
          <h3 className="mono" style={{color:'#e2e8f0',fontSize:16,marginBottom:14}}>üìà Pr√≥ximos Pasos</h3>
          <div style={{textAlign:'left',maxWidth:420,margin:'0 auto',color:'#94a3b8',fontSize:13,lineHeight:2.4}}>
            <div><strong style={{color:'#f59e0b'}}>Semana 1:</strong> Monitorea Google Analytics</div>
            <div><strong style={{color:'#f59e0b'}}>Semana 2-4:</strong> Crea 2-3 backlinks</div>
            <div><strong style={{color:'#f59e0b'}}>Mes 1-2:</strong> Primeros visitantes</div>
            <div><strong style={{color:'#f59e0b'}}>Mes 3:</strong> Primeros ingresos</div>
            <div><strong style={{color:'#f59e0b'}}>Mes 6+:</strong> Ingresos pasivos recurrentes</div>
          </div>
          <div style={{marginTop:28,padding:20,background:'rgba(245,158,11,.06)',border:'1px solid rgba(245,158,11,.15)',borderRadius:14}}>
            <h4 className="mono" style={{color:'#f59e0b',fontSize:14,marginBottom:12}}>üìä Estimaci√≥n Realista de Ingresos</h4>
            <div style={{color:'#94a3b8',fontSize:13,lineHeight:2}}>
              <div>Mes 1-2: <strong style={{color:'#e2e8f0'}}>50-300 visitantes</strong></div>
              <div>Mes 3: <strong style={{color:'#e2e8f0'}}>500-2,000 visitantes</strong></div>
              <div>Mes 6: <strong style={{color:'#e2e8f0'}}>2,000-10,000+/mes</strong></div>
              <div>Ingresos: <strong style={{color:'#22c55e'}}>EUR 300-5,000+/mes</strong></div>
            </div>
          </div>
          <p style={{color:'#64748b',fontSize:14,marginTop:28,fontStyle:'italic'}}>Tu viaje empieza ahora. CREA. Vuelve en 1 mes. üöÄ</p>
          <button onClick={()=>{setCompleted([]);setActive(null)}} style={{marginTop:20,background:'rgba(255,255,255,.06)',border:'1px solid rgba(255,255,255,.1)',color:'#64748b',padding:'10px 24px',borderRadius:10,cursor:'pointer',fontSize:12,fontFamily:'inherit'}}>‚Ü∫ Reiniciar progreso</button>
        </div>}

        {!allDone && <div style={{marginTop:40,background:'rgba(255,255,255,.02)',border:'1px solid rgba(255,255,255,.06)',borderRadius:14,padding:24}}>
          <h3 className="mono" style={{color:'#94a3b8',fontSize:14,marginBottom:16}}>üìä Timeline Estimado</h3>
          <div style={{display:'grid',gridTemplateColumns:'1fr auto',gap:'8px 20px',fontSize:13}}>
            <span style={{color:'#64748b'}}>M√≠nimo (5 URLs)</span><span className="mono" style={{color:'#e2e8f0'}}>2-2.5 horas</span>
            <span style={{color:'#64748b'}}>Est√°ndar (10 URLs)</span><span className="mono" style={{color:'#e2e8f0'}}>2.5-3.5 horas</span>
            <span style={{color:'#64748b'}}>Extenso (20+ URLs)</span><span className="mono" style={{color:'#e2e8f0'}}>4+ horas</span>
          </div>
        </div>}
      </div>
    </div>
  );
}

ReactDOM.createRoot(document.getElementById('app')).render(<App/>);
</script>
