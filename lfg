// ‚îÄ‚îÄ Helpers ‚îÄ‚îÄ
function CopyBtn({ text }) {
  const [c, setC] = useState(false);
  return <button className={`copy-btn${c?' copied':''}`} onClick={(e)=>{e.stopPropagation();navigator.clipboard.writeText(text);setC(true);setTimeout(()=>setC(false),2000)}}>{c?"‚úì Copiado":"üìã Copiar prompt"}</button>;
}

function PromptBlock({ title, prompt, note }) {
  const [open, setOpen] = useState(false);
  return (
    <div className="prompt-block">
      <button className="prompt-toggle" onClick={()=>setOpen(!open)}>
        <span style={{display:'flex',alignItems:'center',gap:10}}><span style={{color:'#f59e0b',fontSize:18}}>üìÑ</span>{title}</span>
        <span style={{transform:open?'rotate(180deg)':'rotate(0)',transition:'transform .3s',fontSize:11,opacity:.4}}>‚ñº</span>
      </button>
      {open && <div className="prompt-body">
        {note && <p style={{color:'#f59e0b',fontSize:13,marginBottom:14,fontStyle:'italic',lineHeight:1.5}}>{note}</p>}
        <div style={{display:'flex',justifyContent:'flex-end',marginBottom:10}}><CopyBtn text={prompt}/></div>
        <pre className="prompt-pre">{prompt}</pre>
      </div>}
    </div>
  );
}

function InfoBox({ color, children }) {
  return <div className="info-box" style={{background:`${color}10`,border:`1px solid ${color}30`}}>{children}</div>;
}

function SectionTitle({ children }) {
  return <h3 className="section-title">{children}</h3>;
}

function StepList({ steps }) {
  return <ol className="step-list">{steps.map((s,i)=><li key={i}>{s}</li>)}</ol>;
}

function CheckItem({ label, checked, onChange }) {
  return (
    <label className="check-item" style={{color:checked?'#22c55e':'#94a3b8'}}>
      <span className="check-box" style={{border:checked?'2px solid #22c55e':'2px solid rgba(255,255,255,.18)',background:checked?'rgba(34,197,94,.12)':'transparent'}}>{checked?"‚úì":""}</span>
      <span style={{textDecoration:checked?'line-through':'none',opacity:checked?.55:1}}>{label}</span>
      <input type="checkbox" checked={checked} onChange={onChange} style={{display:'none'}}/>
    </label>
  );
}

function BadgePopup({ badge, icon, onClose }) {
  return (
    <div className="badge-overlay" onClick={onClose}>
      <div className="badge-card" onClick={e=>e.stopPropagation()}>
        <div style={{fontSize:72,marginBottom:20}}>{icon}</div>
        <div className="mono" style={{color:'#f59e0b',fontSize:12,fontWeight:700,letterSpacing:5,marginBottom:8}}>BADGE DESBLOQUEADO</div>
        <div className="mono" style={{color:'#f1f5f9',fontSize:30,fontWeight:800,marginBottom:28}}>{badge}</div>
        <button onClick={onClose} style={{background:'linear-gradient(135deg,#f59e0b,#d97706)',border:'none',color:'#000',fontWeight:800,padding:'14px 36px',borderRadius:14,cursor:'pointer',fontSize:15,fontFamily:'inherit'}}>¬°SIGUIENTE FASE!</button>
      </div>
    </div>
  );
}

